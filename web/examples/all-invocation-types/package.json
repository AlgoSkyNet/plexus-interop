{
  "name": "@plexus/all-invocation-types-example",
  "version": "0.0.1",
  "description": "JS Client of all possible invocation types example",
  "main": "index.ts",
  "scripts": {
    "gen-proto-js": "pbjs --force-long -t static-module -r plexus -w commonjs -o src/gen/plexus-messages.js ./metadata/Messages.proto",
    "gen-proto-ts": "pbts --force-long -o src/gen/plexus-messages.d.ts src/gen/plexus-messages.js",
    "gen-proto": "npm run gen-proto-js && npm run gen-proto-ts"
  },
  "keywords": [
    "plexus",
    "example",
    "js",
    "client"
  ],
  "dependencies": {
    "@plexus-interop/client": "^0.0.1",
    "@plexus-interop/websocket-transport": "^0.0.1"
  }
}
